    "babel-eslint": "10.1.0",
    "babel-preset-gatsby-package": "2.10.0",
    "commitizen": "4.2.4",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "devmoji": "2.3.0",
    "eslint": "8.11.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-universe": "10.0.0",
    "eslint-plugin-import": "2.25.4",
