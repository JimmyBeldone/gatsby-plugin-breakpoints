{
  "name": "gatsby-plugin-breakpoints",
  "version": "1.3.4",
  "description": "Gatsby plugin providing breakpoints using React Hooks",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-breakpoints",
    "breakpoints",
    "media query",
    "media queries",
    "hooks",
    "responsive"
  ],
  "main": "index.js",
  "author": "Jimmy Beldone <dev.jimmy.beldone@gmail.com>",
  "homepage": "https://github.com/JimmyBeldone/gatsby-plugin-breakpoints",
  "repository": {
    "type": "git",
    "url": "https://github.com/JimmyBeldone/gatsby-plugin-breakpoints.git"
  },
  "bugs": {
    "email": "dev.jimmy.beldone@gmail.com",
    "url": "https://github.com/JimmyBeldone/gatsby-plugin-breakpoints/issues"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint src --fix",
    "pretty": "prettier --write 'src/**/*.js'",
    "test": "echo 'No test specified'",
    "prepare": "cross-env NODE_ENV=production yarn build",
    "build": "babel src --out-dir .",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "babel-eslint": "10.1.0",
    "babel-preset-gatsby-package": "1.13.0",
    "commitizen": "4.2.4",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "devmoji": "2.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-universe": "7.0.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.25.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.3.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "npx --no-install devmoji -e --lint"
    }
  },
  "lint-staged": {
    "{src}/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.15.3"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0 || ^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1 || ^17.0.0"
  }
}
